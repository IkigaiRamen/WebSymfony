{% extends 'back.html.twig' %}

{% block title %}User index{% endblock %}

{% block body %}
    <h1>User index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Email</th>
                <th>Username</th>
                <th>Roles</th>
                <th>Datecreation</th>
                <th>Datemodification</th>
                <th>Job</th>
                <th>Siteweb</th>
                <th>Lastname</th>
                <th>Firstname</th>
                <th>NumTel</th>
                <th>Exp</th>
                <th>Etat</th>
                <th>Sex</th>
                <th>Qualification</th>
                <th>Titre1</th>
                <th>Institut1</th>
                <th>Periode1</th>
                <th>Description1</th>
                <th>Titre2</th>
                <th>Institut2</th>
                <th>Periode2</th>
                <th>Description2</th>
                <th>Work1</th>
                <th>Company1</th>
                <th>Workperiod1</th>
                <th>Workdescription1</th>
                <th>Work2</th>
                <th>Company2</th>
                <th>Qualification1</th>
                <th>Qualification2</th>
                <th>Qualification3</th>
                <th>Qualification4</th>
                <th>Societe</th>
                <th>Workperiod2</th>
                <th>Workdescription2</th>
                <th>Video</th>
                <th>Bio</th>
                <th>City</th>
                <th>Image</th>
                <th>Update_at</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for user in users %}
            <tr>
                <td>{{ user.id }}</td>
                <td>{{ user.email }}</td>
                <td>{{ user.username }}</td>
                <td>{{ user.roles ? user.roles|json_encode : '' }}</td>
                <td>{{ user.datecreation ? user.datecreation|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ user.datemodification ? user.datemodification|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ user.job }}</td>
                <td>{{ user.siteweb }}</td>
                <td>{{ user.lastname }}</td>
                <td>{{ user.firstname }}</td>
                <td>{{ user.numTel }}</td>
                <td>{{ user.exp }}</td>
                <td>{{ user.etat }}</td>
                <td>{{ user.sex }}</td>
                <td>{{ user.qualification }}</td>
                <td>{{ user.titre1 }}</td>
                <td>{{ user.institut1 }}</td>
                <td>{{ user.periode1 }}</td>
                <td>{{ user.description1 }}</td>
                <td>{{ user.titre2 }}</td>
                <td>{{ user.institut2 }}</td>
                <td>{{ user.periode2 }}</td>
                <td>{{ user.description2 }}</td>
                <td>{{ user.work1 }}</td>
                <td>{{ user.company1 }}</td>
                <td>{{ user.workperiod1 }}</td>
                <td>{{ user.workdescription1 }}</td>
                <td>{{ user.work2 }}</td>
                <td>{{ user.company2 }}</td>
                <td>{{ user.qualification1 }}</td>
                <td>{{ user.qualification2 }}</td>
                <td>{{ user.qualification3 }}</td>
                <td>{{ user.qualification4 }}</td>
                <td>{{ user.societe }}</td>
                <td>{{ user.workperiod2 }}</td>
                <td>{{ user.workdescription2 }}</td>
                <td>{{ user.video }}</td>
                <td>{{ user.bio }}</td>
                <td>{{ user.city }}</td>
                <td>{{ user.image }}</td>
                <td>{{ user.updateAt ? user.updateAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('app_user_back_show', {'id': user.id}) }}">show</a>
                    <a href="{{ path('app_user_back_edit', {'id': user.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="43">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_user_back_new') }}">Create new</a>
{% endblock %}
